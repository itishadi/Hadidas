 @model IEnumerable<Hadidas.Models.User>

<h2>Alla användare</h2>

<table>
    <thead>
        <tr>
            <th>Förnamn</th>
            <th>Efternamn</th>
            <th>Email</th>
            <th>Handling</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>
                    <a href="@Url.Action("UpdatedUser", "Hadidas", new { id = user.Id })">Uppdatera</a>
                </td>
            </tr>
        }
    </tbody>
</table>

